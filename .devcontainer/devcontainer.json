{
  "name": "Flutter Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "Dart-Code.flutter",
    "Dart-Code.dart-code"
  ],
  "forwardPorts": [8080],
  "postCreateCommand": "flutter doctor",
  "remoteUser": "root"
}
